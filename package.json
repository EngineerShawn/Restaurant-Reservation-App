    "test:7": "npx npm-run-all -s test:7:*",
    "test:7:front-end": "npm run test:7 --prefix ./front-end",
    "test:7:back-end": "npm run test:7 --prefix ./back-end",
    "test:8": "npx npm-run-all -s test:8:*",
